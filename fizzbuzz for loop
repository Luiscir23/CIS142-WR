#include <iostream>
// fizzbuzz for loop example
// print fizz if the input is multiple by 3
// print buzz if the input is multiple by 5
// print fizzbuzz if both 3 and 5 are multiples
int main() {
	int i; 
	for (i = 1; i <= 100; i++) { // print numbers 1 - 100

		if (i % 3 == 0 && i % 5 == 0) \ { // prints fizzbuzz if number is multiples of 3 and 5
			std::cout << "fizzbuzz\n";
		}
		else if (i % 3 == 0) { // prints fizz if the number is multiple of 3
			std::cout << "fizz\n";
		}
		else if (i % 5 == 0) { // prints buzz if the number is multiple of 5
			std::cout << "buzz\n";
		}
		else {
			std::cout << i << "\n";
		}
	}
}
